<turbine_engine name="DRIEW">
    <milthrust>    40000 </milthrust>
    <maxthrust>   100000 </maxthrust>
    <bypassratio>     0.5 </bypassratio>
    <tsfc>            0.3 </tsfc>
    <bleed>           0.03</bleed>
    <idlen1>         99.0 </idlen1>
    <idlen2>         99.0 </idlen2>
    <maxn1>         100.0 </maxn1>
    <maxn2>         100.0 </maxn2>
    <augmented>         0 </augmented>
    <injected>          0 </injected>

    <function name="IdleThrust">
        <table>
            <independentVar lookup="row">velocities/mach</independentVar>
            <tableData>
                0.0  0
                1.5  1
                2.0  5.5
                2.4  0
            </tableData>
        </table>
    </function>

    <function name="MilThrust">
        <table>
            <independentVar lookup="row">velocities/mach</independentVar>
            <tableData>
                0.0  1
                1.5  0.75
                2.0  0
                2.4  0
            </tableData>
        </table>
    </function>
</turbine_engine>

